{
  "name": "vite",
  "private": true,
  "version": "3.3.2",
  "type": "module",
  "packageManager": "pnpm@9.5.0",
  "scripts": {
    "dev": "vite --mode=mtb --port 7271",
    "build": "vite build --mode=mtb",
    "tsc": "tsc -b",
    "preview": "vite preview --mode=mtb --port 7371",
    "local:dev": "vite --mode=atlocal --port 7271",
    "local:build": "vite build --mode=atlocal",
    "local:preview": "vite preview --mode=atlocal --port 7371",
    "hmh:build": "vite build --mode=hmh",
    "open:build": "vite build --mode=wesley",
    "lint:fix": "eslint --ext .vue,.js,.ts,.tsx ./src --max-warnings 0 --fix --cache",
    "lint": "pnpm lint:tsc && eslint --ext .vue,.js,.ts,.tsx ./src --max-warnings 0 --cache",
    "lint:tsc": "tsc --noEmit"
  },
  "dependencies": {
    "@vue/runtime-dom": "^3.5.13",
    "@vueuse/core": "^10.2.1",
    "core-js": "^3.8.3",
    "crypto-js": "^4.2.0",
    "dayjs": "^1.11.13",
    "echarts": "^5.5.0",
    "exceljs": "^4.4.0",
    "fake-progress": "^1.0.4",
    "js-cookie": "^3.0.5",
    "jsencrypt": "^3.3.2",
    "md5.js": "^1.3.5",
    "nprogress": "^0.2.0",
    "tdesign-icons-vue-next": "^0.3.3",
    "tdesign-vue-next": "^1.10.7",
    "vue": "^3.5.12",
    "vue-router": "^4.4.5"
  },
  "devDependencies": {
    "@types/eslint": "^9.6.1",
    "@types/lodash-es": "^4.17.12",
    "@types/validator": "^13.12.2",
    "@typescript-eslint/eslint-plugin": "^4.33.0",
    "@typescript-eslint/parser": "^4.33.0",
    "@vitejs/plugin-vue": "^5.2.0",
    "@vitejs/plugin-vue-jsx": "^4.1.0",
    "@vue/compiler-sfc": "^3.5.13",
    "@vue/runtime-core": "^3.5.13",
    "babel-eslint": "^10.1.0",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.10.0",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-vue": "^8.7.1",
    "less-loader": "^11.1.3",
    "lodash-es": "^4.17.21",
    "prettier": "2.8.1",
    "sass": "^1.63.6",
    "sass-loader": "^13.3.2",
    "typescript": "~5.8.3",
    "vite": "^5.4.11",
    "vite-plugin-markdown": "^2.2.0",
    "vue-tsc": "^2.1.8"
  },
  "lint-staged": {
    "*.{vue,js,jsx,ts,tsx}": [
      "prettier --write --cache",
      "eslint --fix --cache"
    ]
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not dead",
    "not ie 11"
  ]
}
